<% if (theme.scripts) { %>
    <% theme.scripts.forEach(url => { %>
        <%- ['http://', 'https://', '//'].find(e => url.startsWith(e)) ? `<script src="${url}"></script>` : js(url) %>
    <% }); %>
<% } %>

<% if (theme.stats.busuanzi.enable) { %>
    <script src="<%= theme.stats.busuanzi.script || 'https://busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js' %>" async></script>
<% } %>